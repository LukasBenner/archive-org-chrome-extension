{
  "name": "archive.org extension",
  "description": "Save a website on archive.org",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "archive.org"
  },
  "permissions": [
    "tabs",
    "https://web.archive.org/save/*"
  ],
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  }
}